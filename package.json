{
  "name": "impact",
  "version": "1.1.1",
  "description": "Ghost blogging theme",
  "engines": {
    "ghost": ">=4.0.0",
    "ghost-api": "v4"
  },
  "license": "MIT",
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "scripts": {
    "dev": "gulp",
    "zip": "gulp zip",
    "design": "gulp design"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Blog"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme"
  ],
  "devDependencies": {
    "@tryghost/release-utils": "^0.7.1",
    "autoprefixer": "^10.4.0",
    "beeper": "^3.0.0",
    "bootstrap": "^4.6.0",
    "browser-sync": "^2.27.7",
    "connect-livereload": "^0.6.1",
    "cssnano": "^5.1.7",
    "gscan": "^4.27.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-noop": "^1.0.1",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "handlebars": "^4.7.7",
    "jquery": "^3.6.0",
    "livereload": "^0.9.3",
    "minimist": "^1.2.6",
    "node-sass": "^7.0.1",
    "open": "^8.4.0",
    "postcss": "^8.4.12",
    "postcss-color-mod-function": "^3.0.3",
    "pump": "^3.0.0",
    "sass": "^1.50.1"
  },
  "config": {
    "posts_per_page": 9,
    "custom": {
      "subscribe_homepage_banner": {
        "type": "boolean",
        "default": false,
        "group": "homepage"
      },
      "subscribe_homepage_text": {
        "type": "text",
        "default": "Sign up now to get access members-only posts.",
        "group": "homepage"
      },
      "subscribe_post_banner": {
        "type": "boolean",
        "default": false,
        "group": "post"
      },
      "login_header_link": {
        "type": "boolean",
        "default": false
      },
      "show_author": {
        "type": "boolean",
        "default": true
      }
    }
  }
}
